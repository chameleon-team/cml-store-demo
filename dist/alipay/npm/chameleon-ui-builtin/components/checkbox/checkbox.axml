<view class="cml-checkbox  cml-base cml-view cml-024f99f1" style="{{computedCstyle}}">
  <view class="cml-checkbox-wrap  cml-base cml-view cml-024f99f1" onTap="_cmlEventProxy" data-eventtap="{{['changeCheck']}}">
    <view class="cml-checkbox-check  cml-base cml-view cml-024f99f1" style="{{checkStyle}}">
      <view class="{{ checkClass }}  cml-base cml-view cml-024f99f1">
        <image class="inner-icon  cml-base cml-image cml-024f99f1" src="{{ checkImg }}" />
      </view>
    </view>
    <text class="cml-checkbox-label  cml-base cml-text cml-024f99f1">{{label}}</text>
  </view>
</view>